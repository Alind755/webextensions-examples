{

  "description": "Clear browsing data",
  "manifest_version": 2,
  "name": "clear-browsing-data",
  "version": "2.0",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/clear-browsing-data",
  "icons": {
    "48": "icons/page-48.png"
  },

  "background": {
    "scripts": ["clear-browsing-data.js"]
  },

  "browser_action": {
    "default_icon": "icons/page-32.png"
  },
  
  "permissions": [
    "browsingData"
  ]
}
